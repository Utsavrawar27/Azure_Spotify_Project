{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "df-utsavspotifyproject"
		},
		"AzureDataLakeStorage01_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage01'"
		},
		"AzureSqlDatabase01_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'AzureSqlDatabase01'"
		},
		"AzureDataLakeStorage01_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://utsavstoragespotify.dfs.core.windows.net/"
		},
		"AzureSqlDatabase01_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "utsavspotifyproject.database.windows.net"
		},
		"AzureSqlDatabase01_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "spotifydatabase"
		},
		"AzureSqlDatabase01_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "utsavadmin"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "df-utsavspotifyproject_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDataLakeStorage01_accountKey": {
						"value": "[parameters('AzureDataLakeStorage01_accountKey')]"
					},
					"AzureSqlDatabase01_password": {
						"value": "[parameters('AzureSqlDatabase01_password')]"
					},
					"AzureDataLakeStorage01_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage01_properties_typeProperties_url')]"
					},
					"AzureSqlDatabase01_properties_typeProperties_server": {
						"value": "[parameters('AzureSqlDatabase01_properties_typeProperties_server')]"
					},
					"AzureSqlDatabase01_properties_typeProperties_database": {
						"value": "[parameters('AzureSqlDatabase01_properties_typeProperties_database')]"
					},
					"AzureSqlDatabase01_properties_typeProperties_userName": {
						"value": "[parameters('AzureSqlDatabase01_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}